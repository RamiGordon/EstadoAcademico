<?xml version="1.0" encoding="ISO-8859-1"?>

<entity>
	<name>Barrio</name>
	<clazz>ar.com.osde.seguridad.common.entities.crud.Barrio</clazz>
	<description>barrio</description>
	<defaultOrder>nombre</defaultOrder>
	<maxListElements>6</maxListElements>
	
	<delete>logic</delete>
	
	<customButtons>
		<customButton>
			<title>custom</title>
			<actionUrl>/pages/template/descarga.jsp?decorator=none</actionUrl>
			<imageUrl>/images/iconos/ver.gif</imageUrl>
		</customButton>
	</customButtons>
	
	<fields>
		<primitiveField>
			<name>Código</name>
			<property>codigo</property>
			<columnPreference>
				<orderable>true</orderable>
				<index>1</index>
			</columnPreference>
			<filterPreference>
				<index>1</index>
			</filterPreference>
			<editPreference>
				<index>1</index>
				<changeable>false</changeable>
				<validations>
					<clientValidation>
						<validation>mandatory</validation>
					</clientValidation>
				</validations>
			</editPreference>
			<defaultFilter operator="In" />
		</primitiveField>
	
		<primitiveField>
			<name>Nombre</name>
			<property>nombre</property>
			<columnPreference>
				<orderable>true</orderable>
				<index>2</index>
			</columnPreference>
			<filterPreference>
				<index>2</index>
				<validations>
					<clientValidation>
						<validation>mandatory</validation>
					</clientValidation>
				</validations>
			</filterPreference>
			<editPreference>
				<index>2</index>
				<changeable>true</changeable>
			</editPreference>
			<defaultFilter operator="Like" />
		</primitiveField>

		
	</fields>
</entity>